ce: 0,\r\n            deliveryFee: 0\r\n        }\r\n    },\r\n    created() {\r\n        window.console.log(this.$parent.orders.length)\r\n        this.deliveryInfo = this.$parent.deliveryInfo\r\n        this.items = this.$parent.orders\r\n        if (this.$parent.deliveryInfo.deliveryFee){\r\n            this.deliveryFee = this.$parent.deliveryInfo.deliveryFee\r\n        }\r\n       \r\n\r\n            for (var i=0;i<this.items.length;i++){\r\n                var item = this.items[i]\r\n                if (item.promotionType == 'percent'){\r\n                    this.totalPrice += (item.orderPrice * (100 - item.promotionAmount) / 100)\r\n                }\r\n                if (item.promotionType == 'amount'){\r\n                    this.totalPrice += (item.orderPrice - item.promotionAmount)\r\n                }\r\n                if (item.promotionType == null){\r\n                    this.totalPrice += item.orderPrice \r\n                }\r\n                var optionString = \"\"\r\n                for (var j=0;j<item.menuOptions.length-1;j++){\r\n                    var option = item.menuOptions[j]\r\n                    optionString += option + \", \"\r\n                }\r\n\r\n                optionString += item.menuOptions[item.menuOptions.length-1]\r\n                item.optionString = optionString\r\n            }\r\n            this.$parent.productTotal = this.totalPrice\r\n\r\n\r\n\r\n    },\r\n    methods: {  \r\n        setMode(mode){\r\n            this.$parent.orderMode = mode\r\n            window.localStorage.setItem(\"orderMode\", mode)\r\n            if (mode == 'delivery'){\r\n                this.deliveryFee = this.$parent.deliveryInfo.deliveryFee\r\n            }\r\n            else{\r\n                this.deliveryFee = 0\r\n            }\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n\r\n.roundedCard {\r\n  border-radius: 25px;\r\n  /* border: 1px groove lightgrey ; */\r\n  padding: 0px;\r\n  width: 100%;\r\n  heig