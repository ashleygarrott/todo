                                   <img class=\"py-1 mx-2 my-auto\" v-bind:src=\"'/img/menus/' + items[index].src\" style=\"width:85px;height:85px;object-fit:cover;border-radius: 15%;\">\r\n                                        <div class=\"px-2\">\r\n                                            <p class=\"my-auto\" style=\"font-size:1rem;color:black\"><b>{{item.quantity}} {{item.menuName}}</b></p>\r\n                                            <p v-if=\"item.price == 0\" class=\"my-2 text-secondary\" style=\"font-size:0.8rem\" ><b>{{item.optionString}}</b></p>\r\n                                            <p class=\"my-2 text-muted\" style=\"font-size:0.8rem\" v-for=\"(addOn, index) in item.selectedAddOns\" :key=\"index\">{{addOn}}</p>\r\n                                            <p class=\"my-2 text-muted\" style=\"font-size:0.7rem\" >{{item.menuNotes}}</p>\r\n                                            <p v-if=\"item.promotionType == 'percent'\" class=\"text-muted mb-0\" style=\"font-size:0.8rem\">After {{item.promotionAmount}}% discount</p>\r\n                                            <p v-if=\"item.promotionType == 'amount'\" class=\"text-muted mb-0\" style=\"font-size:0.8rem\">After à¸¿{{item.promotionAmount}} discount</p>\r\n                                        </div>\r\n                                    </div>\r\n                                    <div class=\"my-auto\">\r\n                                        <p v-if=\"item.promotionType == 'percent'\" class=\"my-auto\" style=\"font-size:15px\"><b>{{item.orderPrice * (100 - item.promotionAmount) / 100}}</b></p>\r\n                                        <p v-if=\"item.promotionType == 'amount'\" class=\"my-auto\" style=\"font-size:15px\"><b>{{item.orderPrice - item.promotionAmount}}</b></p>\r\n                                        <p v-if=\"item.promotionType == null\" class=\"my-auto\" style=\"font-size:15px\"><b>{{item.orderPrice}}</b></p>\r\n                                    </div>\r\n